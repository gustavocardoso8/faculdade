Algoritmo "ProcMulti"

procedimento multi (x,y: inteiro)

Var

   aux: inteiro

inicio

aux <- x * y
escreva("A resposta dessa multiplicação é: ", aux)


fimProcedimento

var

p, s: inteiro

Inicio

      Escreva("Escreva um número para iniciarmos: ")
      leia(p)
      Escreva("Agora, informe mais um para multiplicarmos: ")
      leia(s)
      multi(p,s)

Fimalgoritmo